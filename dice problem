#include <stdio.h>

int main()
{ int x,i,j,k;
    printf("enter your sum of three number: ");
    scanf("%d",&x);
    if(x>=3){
    for(i=1;i<7;i++){
        for(j=1;j<7;j++){
            for(k=1;k<7;k++){
                if(i+j+k==x){
                    printf("(%d,%d,%d),",i,j,k);
                }
                
            
            }
        }
    }
}
 else{
     printf("Not possible");
 }
    return 0;
}
